alias ll='ls -l'
alias la='ls -la'

alias gst='git status -s'
alias gd='git diff -U1'
alias gdc='git diff -U1 --cached'
alias gc='git commit -m'
alias gcm='git commit'
alias ga='git add'
alias gco='git checkout'
alias gca='git commit -a -m'
alias gam='git commit --amend'
alias gb='git branch'
alias glp='git log -p -U1'
alias gtr='git tree --all'

alias git-last-files='git log --numstat -10 | grep -E "^(\d|\-)" | cut -f 3 | sort | uniq'

